{
  "manifest_version": 2,
  "name": "Word Meaning",
  "version": "0.1",
  "permissions" : ["storage"],
  "content_scripts": [
    {
      "matches":[
        "<all_urls>"
      ],
      "js": [
        "content.js"
        ]
    }
  ],
  "background" : {
    "scripts" : ["background.js"]
  },
    "browser_action" : {
      "default_icon" : "Ashoka_Chakra.png",
      "default_popup" : "popup/popup.html",
      "default_title" : "popup text"
  }

}
